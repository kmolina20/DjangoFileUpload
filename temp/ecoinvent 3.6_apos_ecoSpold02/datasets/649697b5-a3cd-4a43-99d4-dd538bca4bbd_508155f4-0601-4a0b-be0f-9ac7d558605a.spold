        <activityName xml:lang="en">dewatering of ethanol from biomass, from 95% to 99.7% solution state</activityName>
        <synonym xml:lang="en">grain alcohol</synonym>
        <synonym xml:lang="en">fermentation alcohol</synonym>
        <synonym xml:lang="en">ethyl hydrate</synonym>
        <synonym xml:lang="en">Ethyl alcohol</synonym>
