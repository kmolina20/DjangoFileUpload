        <property propertyId="38f94dd1-d5aa-41b8-b182-c0c42985d9dc" variableName="transport_lorry_RP_price_EUR2005" amount="0.0214781215378474" unitId="7b75baf0-cbee-45e6-bf66-af119da06553">
