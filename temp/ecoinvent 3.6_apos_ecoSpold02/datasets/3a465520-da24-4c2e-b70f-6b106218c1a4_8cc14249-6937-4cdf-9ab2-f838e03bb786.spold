    <activityDescription>
      <activity id="3a465520-da24-4c2e-b70f-6b106218c1a4" activityNameId="8a5ed080-9837-41c6-bb8c-b6d3a901dbd4" parentActivityId="e9cc29fc-c3ff-4bec-99bf-a9fbee3d1b89" inheritanceDepth="-1" type="1" specialActivityType="0" datasetIcon="https://db3.ecoinvent.org/images/3882ea81-1510-459d-ad01-f7b96c9753ba">
        <activityName xml:lang="en">fish canning plant construction and maintenance</activityName>
        <includedActivitiesStart xml:lang="en">From construction of the plant, through maintenance.</includedActivitiesStart>
        <includedActivitiesEnd xml:lang="en">End of life treatment of the plant</includedActivitiesEnd>
        <generalComment>
          <text xml:lang="en" index="1">The activity represents the construction and maintenance along its lifetime of a fish canning (including fish pre-processing) plant, expressed as the installed capacity required to process 1000 kg of raw material. The life cycle inventories were produced based on a sample of 1 Peruvian plant (processing capacity: 461 t/y), 1 Ecuadorian plant (61 000 t/y) and data from other industry representatives. This dataset may be adapted to other global production sites.</text>
          <imageUrl index="3">https://db3.ecoinvent.org/images/b933f84e-878a-413c-808d-439e6e21c9f1</imageUrl>
          <text xml:lang="en" index="2">System boundaries of fish processing plants and relations between activities</text>
        </generalComment>
      </activity>
      <classification classificationId="fb9a6a31-ef0a-4542-ab6c-67487f4f196f">
        <classificationSystem xml:lang="en">ISIC rev.4 ecoinvent</classificationSystem>
        <classificationValue xml:lang="en">1020:Processing and preserving of fish, crustaceans and molluscs</classificationValue>
      </classification>
      <geography geographyId="7846b897-7c04-4f9f-b607-2b83fcd9a74c">
        <shortname xml:lang="en">RoW</shortname>
        <comment>
          <text xml:lang="en" index="1">Based on Peruvian data</text>
        </comment>
      </geography>
      <technology technologyLevel="3">
        <comment>
          <imageUrl index="5">https://db3.ecoinvent.org/images/6a1376fb-029a-40c4-bf7c-454be0be11c8</imageUrl>
          <imageUrl index="3">https://db3.ecoinvent.org/images/b812ba67-ea18-4027-a9fd-f5b02b98cc6e</imageUrl>
          <text xml:lang="en" index="2">Process diagram of large fish canning</text>
          <text xml:lang="en" index="1">Construction and maintenance activities of Peruvian and Ecuadorian fish canning plants, per t of processing capacity. Plants have a lifespan of 40 years.</text>
          <text xml:lang="en" index="4">Process diagram of small fish canning</text>
        </comment>
      </technology>
      <timePeriod startDate="2011-01-01" endDate="2019-12-31" isDataValidForEntirePeriod="true">
        <comment>
          <text xml:lang="en" index="1">Production figures represent the period 2012-2014. Valid as of 2017.</text>
        </comment>
      </timePeriod>
      <macroEconomicScenario macroEconomicScenarioId="d9f57f0a-a01f-42eb-a57b-8f18d6635801">
        <name xml:lang="en">Business-as-Usual</name>
        <comment xml:lang="en">Fish caninng in Peru, a large global producer of canned fish.  Fish caninng in Ecuador, the second largest global tuna processor after Thailand. </comment>
      </macroEconomicScenario>
